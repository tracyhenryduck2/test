package com.zt.supermarket.bean;

import com.avatar.db.annotation.*;
import com.common.*;

/**
 * 超市
 * @author xin.chou
 *
 */
@Table(name="market")
public class MarketBean {

	/**
	 *  
	*/
	@Column(name="id",primaryKey=true,generatorType=GeneratorType.AUTO_INCREMENT)
	private Long id;

	/**
	 * 名称 
	*/
	@Column(name="name")
	private String name;

	/**
	 * 排名 
	*/
	@Column(name="rank")
	private Long rank;
	
	@Column(name="type")
	private Long type;

	/**
	 * 图片 
	*/
	@Column(name="head_pic")
	private String headPic;

	/**
	 * 描述 
	*/
	@Column(name="info")
	private String info;
    	
    /** 
     * 
     * @return 
     */ 
    public Long getId() {
    	return id;
    }
    
    /** 
     * 
     * @param 
     */ 
    public void setId(Long id) {
    	this.id = id;
    }
    	
    /** 
     * 名称
     * @return 
     */ 
    public String getName() {
    	return name;
    }
    
    /** 
     * 名称
     * @param 
     */ 
    public void setName(String name) {
    	this.name = name;
    }
    	
    /** 
     * 排名
     * @return 
     */ 
    public Long getRank() {
    	return rank;
    }
    
    /** 
     * 排名
     * @param 
     */ 
    public void setRank(Long rank) {
    	this.rank = rank;
    }
    
    
    public Long getType() {
		return type;
	}

	public void setType(Long type) {
		this.type = type;
	}

	/** 
     * 图片
     * @return 
     */ 
    public String getHeadPic() {
    	return headPic;
    }
    
    /** 
     * 图片
     * @param 
     */ 
    public void setHeadPic(String headPic) {
    	this.headPic = headPic;
    }
    	
    /** 
     * 描述
     * @return 
     */ 
    public String getInfo() {
    	return info;
    }
    
    /** 
     * 描述
     * @param 
     */ 
    public void setInfo(String info) {
    	this.info = info;
    }
}
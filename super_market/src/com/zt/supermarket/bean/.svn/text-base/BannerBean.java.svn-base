package com.zt.supermarket.bean;

import com.avatar.db.annotation.Column;
import com.avatar.db.annotation.GeneratorType;
import com.avatar.db.annotation.Table;

/**
 * 首页banner
 * @author xin.chou
 *
 */
@Table(name="banner")
public class BannerBean {

	/**
	 *  
	*/
	@Column(name="id",primaryKey=true,generatorType=GeneratorType.AUTO_INCREMENT)
	private Long id;

	/**
	 * 名称 
	*/
	@Column(name="filename")
	private String filename;

	/**
	 * 链接地址 
	*/
	@Column(name="url")
	private String url;

	/**
	 * 排序 
	*/
	@Column(name="seq_no")
	private Long seqNo;
    	
    /** 
     * 
     * @return 
     */ 
    public Long getId() {
    	return id;
    }
    
    /** 
     * 
     * @param 
     */ 
    public void setId(Long id) {
    	this.id = id;
    }
    	
    /** 
     * 名称
     * @return 
     */ 
    public String getFilename() {
    	return filename;
    }
    
    /** 
     * 名称
     * @param 
     */ 
    public void setFilename(String filename) {
    	this.filename = filename;
    }
    	
    /** 
     * 链接地址
     * @return 
     */ 
    public String getUrl() {
    	return url;
    }
    
    /** 
     * 链接地址
     * @param 
     */ 
    public void setUrl(String url) {
    	this.url = url;
    }
    	
    /** 
     * 排序
     * @return 
     */ 
    public Long getSeqNo() {
    	return seqNo;
    }
    
    /** 
     * 排序
     * @param 
     */ 
    public void setSeqNo(Long seqNo) {
    	this.seqNo = seqNo;
    }
}